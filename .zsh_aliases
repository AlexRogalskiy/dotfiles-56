######################
### CUSTOM ALIASES ###
######################
alias zshconfig="vim ~/.zshrc"
alias sourcezsh="source ~/.zshrc"
alias ohmyzsh="vim ~/.oh-my-zsh"
alias bashconfig="vim ~/.bash_profile"
alias sourcebash="source ~/.bash_profile"
# convenvience
alias home="cd ~"
alias c="clear"
alias h="history"
alias rm="rm -i"
alias brewery="brew update && brew upgrade && brew cleanup"
alias python="python3"
alias mci="mvn clean install"
alias mcifast="mvn clean install -DskipTests"
# npm
alias nrun="npm run dev"
alias nins="npm install"
alias nbld="npm run build"
# killswitches
alias killfirefox="pkill -9 firefox"
# wttr
alias wttr="curl -4 http://www.wttr.in/Oslo"
alias wttrHome="curl -4 http://www.wttr.in/Rjukan"
alias moon="curl -4 http://www.wttr.in/Moon"
